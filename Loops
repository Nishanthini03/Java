public class Main {
  public static void main(String[] args) {
    System.out.println("This ïs  for loop");
    int i=1;
    for(i; i<11; i++) {
       System.out.println("This is " + i + "th iteration.");
    }
    System.out.println("This ïs  while loop"); 
    while(i<10){
      System.out.println(i);
    }
  }
}
