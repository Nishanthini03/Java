// Defining the class
class Car {
    String brand;
    int year;

    // Constructor
    Car(String b, int y) {
        brand = b;
        year = y;
    }

    // Method to display car details
    void display() {
        System.out.println("Car Brand: " + brand);
        System.out.println("Year: " + year);
    }
}

public class Main {
    public static void main(String[] args) {
        // Creating objects with constructor
        Car car1 = new Car("Honda", 2021);
        Car car2 = new Car("Ford", 2023);

        // Calling method on objects
        car1.display();
        car2.display();
    }
}
